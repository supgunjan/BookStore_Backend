{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "comma-dangle": [0],
    "func-names": [0],
    "strict": [0],
    "vars-on-top": [0],
    "max-len": [0],
    "no-param-reassign": [0],
    "no-shadow": [0],
    "object-shorthand": [0],
    "no-underscore-dangle": [0],
    "newline-per-chained-call": [0],
    "new-cap": [0],
    "no-else-return": [0],
    "consistent-return": [0],
    "prefer-rest-params": [0]
  }
}
